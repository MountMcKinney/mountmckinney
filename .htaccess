RewriteEngine On
RewriteRule ^campfirestories.php$ /campfirestories/
RewriteRule ^twocents.php$ /twocents/
RewriteRule ^subscribe.php$ /subscribe/
RewriteRule ^contact.php$ /contact/
RewriteRule ^portfolio.php$ /portfolio/

php_flag  display_errors        on
php_value error_reporting       2039


